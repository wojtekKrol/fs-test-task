{
  "name": "fe",
  "version": "1.0.0",
  "description": "Frontend part of recruitment task",
  "author": "Wojtas",
  "license": "MIT",
  "scripts": {
    "postinstall": "husky",
    "dev": "vite",
    "build": "tsc && vite build",
    "test": "jest",
    "serve": "vite preview",
    "commit": "commit",
    "lint": "yarn lint:fix && yarn lint:check",
    "lint:fix": "yarn lint:format && yarn lint:fix:code",
    "lint:format": "prettier --write --ignore-unknown .",
    "lint:fix:code": "eslint --fix .",
    "lint:check": "yarn lint:code && yarn lint:text",
    "lint:code": "eslint --cache --cache-location node_modules/.cache/eslint --cache-strategy content --ext .ts .",
    "lint:text": "prettier --check \"**/*.{json,md,mdx,yml}\""
  },
  "dependencies": {
    "@headlessui/react": "^1.7.13",
    "axios": "^1.6.8",
    "chalk": "^5.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "react-feather": "^2.0.10",
    "vite-plugin-svgr": "2.4.0"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.19.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.0.1",
    "autoprefixer": "^10.4.19",
    "classnames": "^2.3.2",
    "eslint": "^8.31.0",
    "eslint-plugin-jest-dom": "^4.0.3",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-testing-library": "^5.9.1",
    "husky": "^8.0.3",
    "jest-environment-jsdom": "^29.3.1",
    "postcss": "^8.4.38",
    "react-test-renderer": "^18.2.0",
    "tailwindcss": "^3.4.3",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^4.0.3"
  }
}
